{
    "manifest_version": 3,
    "name": "Project Cactus",
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "description": "A cross-platform AI Fake News Detector.",
    "version": "0.1.0",
    "background": {
        "service_worker": "background.js"
    },
    "host_permissions": [
        "<all_urls>"
    ],
    "icons": {
        "16": "/images/ProjectCactus2.png",
        "32": "/images/ProjectCactus2.png",
        "48": "/images/ProjectCactus2.png",
        "128": "/images/ProjectCactus2.png"
    }
}